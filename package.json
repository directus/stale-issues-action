		"@octokit/graphql": "8.1.1",
		"dayjs": "1.11.12",
		"eslint": "9.8.0",
		"eslint-plugin-format": "0.1.2",
		"tsup": "8.2.4",
		"typescript": "5.5.4"
	}
}
